<% @title ="職員(しょくいん staff member)アカウントの編集(へんしゅうedit)"%>
<h1><%= @title %></h1>

<div id="login-form">
	<%= form_with model: @staff_member, url: [ :admin, @staff_member ] do |f| %>

	<%= render "form", f: f %>
	<div class='buttons'>
		<%= f.submit "更新" %>
		<%= link_to "キャンセル", admin_staff_members_path%>
	</div>
	<% end %>
</div>