<% @title = "顧客管理 新規登録" %>
<h1><%= @title %></h1>

<div id="login-form">
  <%= form_with model: @customer_form, scope: "form", url: staff_customers_path do |f| %>
      <%= render "form", f: f %>

      <div class="buttons">
        <%= f.submit "登録" %>
        <%= link_to "キャンセル", staff_customers_path %>
      </div>
  <% end %>
</div>