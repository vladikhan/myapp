<!DOCTYPE html>
<html>
  <head>
    <title>Admin Area</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag "application", media: "all" %>
<%= javascript_include_tag "application", "data-turbo-track": "reload", defer: true %>
  </head>
  
  <body>
    <div id="wrapper">
      <%= render "admin/shared/header" %>
      <div id="container">
        <%= yield %>
      </div>
      <%= render "shared/footer" %>
    </div>
  </body>
</html>
